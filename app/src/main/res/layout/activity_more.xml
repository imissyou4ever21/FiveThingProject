<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:fillViewport="false"
    tools:context="com.example.fivethingproject.More">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.fivethingproject.More">


        <ImageView
            android:id="@+id/derazan"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/derazan"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namederazan"
            android:gravity="center"
            android:padding="8dp"
            android:text="DeMar DeRozan"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="174dp"
            android:onClick="namederazan"/>

        <ImageView
            android:id="@+id/cousins"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/cousins"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namecousins"
            android:gravity="center"
            android:padding="8dp"
            android:text="DeMarcus Cousins"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="12dp"
            android:onClick="namecousins"
            android:layout_alignLeft="@+id/cousins"
            android:layout_alignStart="@+id/cousins"
            android:layout_alignRight="@+id/cousins"
            android:layout_alignEnd="@+id/cousins"
            android:layout_alignTop="@+id/namederazan"
            android:layout_alignBottom="@+id/namederazan" />




        <ImageView
            android:id="@+id/duncan"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/duncan"
            android:layout_below="@+id/derazan"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/nameduncan"
            android:gravity="center"
            android:padding="8dp"
            android:text="Tim Duncan"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="nameduncan"
            android:layout_alignBottom="@+id/george"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="175dp" />

        <ImageView
            android:id="@+id/durant"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/durant"
            android:layout_alignTop="@+id/duncan"
            android:layout_alignLeft="@+id/namecousins"
            android:layout_alignStart="@+id/namecousins" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namedurant"
            android:gravity="center"
            android:padding="8dp"
            android:text="Kevin Durant"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namedurant"
            android:layout_alignTop="@+id/nameduncan"
            android:layout_alignLeft="@+id/durant"
            android:layout_alignStart="@+id/durant" />

        <ImageView
            android:id="@+id/george"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/durant"
            android:scaleType="fitCenter"
            android:src="@drawable/george" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namegeorge"
            android:gravity="center"
            android:padding="8dp"
            android:text="Paul George"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namegeorge"
            android:layout_below="@+id/namedurant"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="151dp" />

        <ImageView
            android:id="@+id/griffin"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/griffin"
            android:layout_alignTop="@+id/george"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namegriffin"
            android:gravity="center"
            android:padding="8dp"
            android:text="Blake Griffin"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namegriffin"
            android:layout_alignTop="@+id/namegeorge"
            android:layout_alignLeft="@+id/griffin"
            android:layout_alignStart="@+id/griffin" />

        <ImageView
            android:id="@+id/hardens"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/hardens"
            android:layout_below="@+id/george"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namehardens"
            android:gravity="center"
            android:padding="8dp"
            android:text="James Harden"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namehardens"
            android:layout_below="@+id/namegeorge"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="152dp" />

        <ImageView
            android:id="@+id/irving"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/irving"
            android:layout_alignTop="@+id/hardens"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/nameirving"
            android:gravity="center"
            android:padding="8dp"
            android:text="Kyrie Irving"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="nameirving"
            android:layout_alignTop="@+id/namehardens"
            android:layout_alignLeft="@+id/irving"
            android:layout_alignStart="@+id/irving" />

        <ImageView
            android:id="@+id/kobe"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/irving"
            android:scaleType="fitCenter"
            android:src="@drawable/kobe" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namekobe"
            android:gravity="center"
            android:padding="8dp"
            android:text="Kobe Bryant"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namekobe"
            android:layout_marginTop="153dp"
            android:layout_below="@+id/namehardens"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/lowry"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/lowry"
            android:layout_below="@+id/irving"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namelowry"
            android:gravity="center"
            android:padding="8dp"
            android:text="Kyle Lowry"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namelowry"
            android:layout_alignTop="@+id/namekobe"
            android:layout_alignLeft="@+id/lowry"
            android:layout_alignStart="@+id/lowry" />


        <ImageView
            android:id="@+id/parker"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/lowry"
            android:scaleType="fitCenter"
            android:src="@drawable/parker" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/nameparker"
            android:gravity="center"
            android:padding="8dp"
            android:text="Tony Parker"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="nameparker"
            android:layout_marginTop="152dp"
            android:layout_below="@+id/namekobe"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/rose"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/rose"
            android:layout_below="@+id/lowry"
            android:layout_alignLeft="@+id/lowry"
            android:layout_alignStart="@+id/lowry" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namerose"
            android:gravity="center"
            android:padding="8dp"
            android:text="Derrick Rose"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namerose"
            android:layout_alignTop="@+id/nameparker"
            android:layout_alignLeft="@+id/rose"
            android:layout_alignStart="@+id/rose" />

        <ImageView
            android:id="@+id/thompson"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/rose"
            android:scaleType="fitCenter"
            android:src="@drawable/thompson" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namethompson"
            android:gravity="center"
            android:padding="8dp"
            android:text="Klay Thompson"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namethompson"
            android:layout_marginTop="152dp"
            android:layout_below="@+id/nameparker"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/wade"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:scaleType="fitCenter"
            android:src="@drawable/wade"
            android:layout_below="@+id/rose"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/namewade"
            android:gravity="center"
            android:padding="8dp"
            android:text="Dwyane Wade"
            android:fontFamily="sans-serif-medium"
            android:textColor="#000000"
            android:textSize="14dp"
            android:onClick="namewade"
            android:layout_alignTop="@+id/namethompson"
            android:layout_alignLeft="@+id/wade"
            android:layout_alignStart="@+id/wade" />


    </RelativeLayout>
</ScrollView>